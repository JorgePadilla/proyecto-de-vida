<h1>Listing nota_entregas</h1>

<table>
  <tr>
    <th>Fecha</th>
    <th>Numero</th>
    <th>Asesor</th>
    <th>Producto</th>
    <th>Cantidad</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @nota_entregas.each do |nota_entrega| %>
  <tr>
    <td><%= nota_entrega.fecha %></td>
    <td><%= nota_entrega.numero %></td>
    <td><%= nota_entrega.asesor_id %></td>
    <td><%= nota_entrega.producto_id %></td>
    <td><%= nota_entrega.cantidad %></td>
    <td><%= link_to 'Show', nota_entrega %></td>
    <td><%= link_to 'Edit', edit_nota_entrega_path(nota_entrega) %></td>
    <td><%= link_to 'Destroy', nota_entrega, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

